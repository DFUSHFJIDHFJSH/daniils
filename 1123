#include <iostream>
using namespace std;

int main() {


	int i = 0;


	do {
		i++;
	  cout << i;
	} while (i <= 8);

		return 0;
}

#include <iostream>
using namespace std;

int main() {
	int i = 0;
	string username = "Daniils", u1;
	do {
		i++;
	  cin >> u1;
	  if (u1 == username) { exit(0); }
	} while (i < 5);
	cout << "out of attempts";
		return 0;
}


